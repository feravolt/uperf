            },
            {
                "hint": "Wakeup",
                "action": "heavyLoad",
                "maxDuration": 1500,
                "note": "屏幕点亮Hint"
            }
        ],
        "stateTransition": {
            "note": "Hint状态转移表，key为源状态，value为目的状态列表（按优先级降序排列），不含状态退出转移",
