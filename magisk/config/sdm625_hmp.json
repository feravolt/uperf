                "HeavyLoad"
            ],
            "Pressed": [
                "SsAnim",
                "SfBoost"
            ],
            "Tap": [
                "SsAnim",
                "SfLag",
                "HeavyLoad"
            ],
            "Swipe": [
                "SsAnim",
                "SfLag",
                "HeavyLoad"
            ],
            "HeavyLoad": [
                "SfBoost"
            ],
            "SfLag": [],
            "SfBoost": [],
            "SsAnim": [
                "SfBoost"
            ],
            "Wakeup": [
                "SfBoost"
            ]
        }
    },
